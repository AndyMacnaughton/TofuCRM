
<div class="wrapper">

  <%= form_tag sessions_path do %>
    <h1>Log in</h1>
    <div class="field">
      <%= text_field_tag :email, params[:email], :placeholder => "Email" %>
    </div>
    <div class="field">
      <%= password_field_tag :password, :placeholder => "Email"  %>
    </div>
    <div class="actions"><%= submit_tag "Log in" %></div>
  <% end %>
</div>
